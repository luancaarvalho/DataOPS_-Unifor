{
  "id": null,
  "uid": "covid-dashboard",
  "title": "Monitoramento COVID Ceará",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 2,
  "style": "dark",
  "refresh": "1m",
  "panels": [
    {
      "title": "Evolução de Casos Confirmados por Cidade",
      "type": "timeseries",
      "datasource": {
        "type": "postgres",
        "uid": "covid_postgres"
      },
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT date AS time, city, confirmed AS value FROM covid_cases ORDER BY date;",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "showPoints": "never",
            "spanNulls": true,
            "lineWidth": 2
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 0 }
    }
  ]
}
